<!-- Managed by Ansible - do not edit manually -->
<!-- HTTP Connector for polywiki -->
<Connector port="{{ polywiki_tomcat_http_port }}" 
           protocol="HTTP/1.1"
           connectionTimeout="20000"
           redirectPort="8443"
           maxThreads="200"
           minSpareThreads="10"
           enableLookups="false"
           acceptCount="100"
           URIEncoding="UTF-8"
           useBodyEncodingForURI="true"
           proxyName="{{ polywiki_apache_server_name }}"
           proxyPort="443"
           scheme="https"
           secure="true" />

